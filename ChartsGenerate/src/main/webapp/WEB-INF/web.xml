<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>RTIDataAgg</servlet-name>
  	<display-name>RTIDataAgg</display-name>
  	<description></description>
  	<servlet-class>com.otsi.rti.controller.RTIDataAgg</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>RTIInitialDataAgg</servlet-name>
  	<display-name>RTIInitialDataAgg</display-name>
  	<description></description>
  	<servlet-class>com.otsi.rti.controller.RTIInitialDataAgg</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>GetIndices</servlet-name>
  	<display-name>GetIndices</display-name>
  	<description></description>
  	<servlet-class>com.otsi.rti.controller.GetIndices</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>RTIInitialDataAgg</servlet-name>
  	<url-pattern>/RTIInitialDataAgg</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>RTIDataAgg</servlet-name>
  	<url-pattern>/RTIDataAgg</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>GetIndices</servlet-name>
  	<url-pattern>/GetIndices</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  <welcome-file>chartVisu.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>
